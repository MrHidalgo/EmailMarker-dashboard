// SECTION modal api remove :: START

mixin modalContent()
	a.c-modal__close(href="#" class="" title="" popup-close-js)
		i.icon-font.icon-close
	.c-modal__header
		p Delete Api
	.c-modal__body
		p Are you sure you want to delete this Api Key ?
	.c-modal__footer
		a(href="#" class="" title="")!= "Delete"

+modal('apiRemove', 'apiRemove', 'modalContent')

// SECTION modal api remove :: END
